{
  "dashboard": {
    "id": null,
    "uid": "skippy-pulse-v1",
    "title": "Skippy Pulse Dashboard",
    "tags": [
      "skippy",
      "pulse"
    ],
    "timezone": "browser",
    "schemaVersion": 30,
    "version": 1,
    "refresh": "10s",
    "panels": [
      {
        "type": "graph",
        "title": "Pulse Emissions by Type",
        "targets": [
          {
            "expr": "pulse_emitted_total",
            "legendFormat": "{{type}} - {{lane}}",
            "interval": ""
          }
        ],
        "datasource": "Prometheus",
        "gridPos": {
          "x": 0,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "type": "graph",
        "title": "Pulse Latency by Type",
        "targets": [
          {
            "expr": "pulse_latency_seconds",
            "legendFormat": "{{type}}",
            "interval": ""
          }
        ],
        "datasource": "Prometheus",
        "gridPos": {
          "x": 12,
          "y": 0,
          "w": 12,
          "h": 8
        }
      },
      {
        "type": "graph",
        "title": "Rejected Pulses",
        "targets": [
          {
            "expr": "pulse_rejected_total",
            "legendFormat": "{{reason}}",
            "interval": ""
          }
        ],
        "datasource": "Prometheus",
        "gridPos": {
          "x": 0,
          "y": 9,
          "w": 24,
          "h": 8
        }
      }
    ]
  },
  "overwrite": true
}